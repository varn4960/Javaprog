# Javaprog
public class TriangularPattern{
	public static void main(String[]args){
	    
	        int i=1;
		    while(i<=3){
		        int j=1;
		        while(j<i){
		            System.out.print("f");
		            j++;
		           
		        }
		        System.out.println("f");
		        i++;
		    
		}
