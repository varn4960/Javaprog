mport java.util.*;

class Solution {

    public static void main(String args[]) {
        Scanner in = new Scanner(System.in);
        int count = in.nextInt();
        int res = 0;
        for (int i = 0; i < count; i++) {
            String unary = in.next();
            res += unary.length();
        }

        for (int i=0; i<res; i++) {
            System.out.print("1");
        }
        
    }
}
