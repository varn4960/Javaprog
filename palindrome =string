import java.util.*;
public class Palindromestring {
	public static void main(String[]args) {
		Scanner sc=new Scanner(System.in);
		String s=sc.next();
		String x=s;
		String rev=" ";
		int len=s.length();
		for(int i=len-1;i>=0;i--) {
			rev=rev+s.charAt(i);
			}
		if(rev==x) {
			System.out.println(s+"P");
			
			
		}
		else {
			System.out.println(s+"not a P");
			
		}
